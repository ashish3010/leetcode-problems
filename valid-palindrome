JavaScript---

https://leetcode.com/problems/valid-palindrome/
Easy---
Time Complexity -- O(n)

var isPalindrome = function(s) {
    const y = s.toLowerCase().replace(/[^\w\s]|_/g, '')
    const x = y.split(' ').join('')
    let newStr = []

    let i = x.length - 1;
    while(i >= 0){
        newStr.push(x[i])
        i--
    }

    const res = newStr.join('')
    return res === x
};
